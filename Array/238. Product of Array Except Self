/**
 * @param {number[]} nums
 * @return {number[]}
 */
var productExceptSelf = function(nums) { //BF . O(n^2)
    let res = [];
    let len = nums.length;
    for(let i = 0 ; i < len; i++){
        let product = 1;
        for(let j = 0; j < len ; j++){
            if(j != i){
                product *=nums[j];
            }  
        }
        res.push(product);
    }
    return res;
};


